 = data.links?.directions;\n\n  cardsHtml += `\n    <div class=\"card\">\n      <h2>${title}</h2>\n      <p class=\"rating\">‚≠ê <strong>${rating}</strong> (${reviews} avalia√ß√£o${reviews === 1 ? '' : 'es'})</p>\n      <p><span class=\"icon\">üìç</span> ${address}</p>\n      <p><span class=\"icon\">üìû</span> ${phone}</p>\n      <p><span class=\"icon\">‚è∞</span> ${hours}</p>\n      ${website ? `<p><span class=\"icon\">üåê</span> <a href=\"${website}\" target=\"_blank\">${website}</a></p>` : ''}\n      ${maps ? `<p><span class=\"icon\">üó∫Ô∏è</span> <a href=\"${maps}\" target=\"_blank\">Ver no mapa</a></p>` : ''}\n    </div>\n  `;\n}\n\nconst finalHtml = `\n<!DOCTYPE html>\n<html lang=\"pt-BR\">\n<head>\n  <meta charset=\"UTF-8\" />\n  <title>Relat√≥rio de Concorrentes</title>\n  <style>\n    body {\n      font-family: 'Segoe UI', sans-serif;\n      background: linear-gradient(to right, #e3f2fd, #fce4ec);\n      padding: 40px;\n      margin: 0;\n    }\n\n    h1 {\n      text-align: center;\n      color: #1e88e5;\n      margin-bottom: 30px;\n      font-size: 2.2em;\n    }\n\n    .card {\n      background: white;\n      border-radius: 16px;\n      padding: 24px;\n      margin: 0 auto 30px auto;\n      max-width: 700px;\n      box-shadow: 0 6px 12px rgba(0,0,0,0.1);\n      transition: transform 0.2s ease;\n    }\n\n    .card:hover {\n      transform: scale(1.02);\n    }\n\n    .card h2 {\n      margin-top: 0;\n      font-size: 1.5em;\n      color: #37474f;\n    }\n\n    .rating {\n      font-size: 1.1em;\n      margin-bottom: 10px;\n      color: #fbc02d;\n    }\n\n    p {\n      margin: 6px 0;\n      font-size: 0.96em;\n      color: #455a64;\n    }\n\n    .icon {\n      margin-right: 6px;\n      color: #1e88e5;\n    }\n\n    a {\n      color: #039be5;\n      text-decoration: none;\n    }\n\n    a:hover {\n      text-decoration: underline;\n    }\n\n    button {\n      display: block;\n      margin: 0 auto 40px auto;\n      padding: 12px 24px;\n      background-color: #1e88e5;\n      color: white;\n      font-size: 1em;\n      border: none;\n      border-radius: 8px;\n      cursor: pointer;\n      box-shadow: 0 4px 10px rgba(0,0,0,0.1);\n    }\n\n    button:hover {\n      background-color: #1565c0;\n    }\n  </style>\n</head>\n<body>\n  <h1>Relat√≥rio de Cl√≠nicas Veterin√°rias em Macei√≥</h1>\n\n  <button onclick=\"exportPDF()\">üìÑ Exportar como PDF</button>\n\n  ${cardsHtml}\n\n  <script src=\"https://cdnjs.cloudflare.com/ajax/libs/html2pdf.js/0.10.1/html2pdf.bundle.min.js\"></script>\n  <script>\n    function exportPDF() {\n      const element = document.body;\n      const opt = {\n        margin:       0.5,\n        filename:     'relatorio-clinicas.pdf',\n        image:        { type: 'jpeg', quality: 0.98 },\n        html2canvas:  { scale: 2 },\n        jsPDF:        { unit: 'in', format: 'letter', orientation: 'portrait' }\n      };\n      html2pdf().from(element).set(opt).save();\n    }\n  </script>\n</body>\n</html>\n`;\n\nreturn [{\n  json: {\n    html: finalHtml\n  }\n}];\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[480,1008],"id":"27f05c1a-5594-4eab-acaa-7b42e42e23d0","name":"Code","disabled":true},{"parameters":{"path":"bbe19d53-bc14-478f-8f0c-415ec404a1c1","responseMode":"responseNode","options":{}},"type":"n8n-nodes-base.webhook","typeVersion":2,"position":[144,672],"id":"3a2583b9-3c1b-4795-ae30-862564126570","name":"Webhook","webhookId":"bbe19d53-bc14-478f-8f0c-415ec404a1c1","disabled":true},{"parameters":{"html":"{{ $json.propertyName }}"},"type":"n8n-nodes-base.html","typeVersion":1.2,"position":[592,672],"id":"0606ede8-6443-478b-816b-db863ad4f2fe","name":"HTML","disabled":true},{"parameters":{"assignments":{"assignments":[{"id":"985b3855-89db-46e0-95d8-4bc07dae9edd","name":"teste","value":"={{ $json.html }}","type":"string"}]},"options":{}},"type":"n8n-nodes-base.set","typeVersion":3.4,"position":[704,1008],"id":"ec0cfe9d-ed05-4452-ad1f-e516cd5cb9d2","name":"Edit Fields1","disabled":true},