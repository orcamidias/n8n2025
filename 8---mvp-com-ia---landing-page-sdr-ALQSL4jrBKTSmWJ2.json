     chat.appendChild(wrap); chat.scrollTop = chat.scrollHeight;\n    }\n    if(!show && t){ t.remove(); }\n  }\n\n  // Sequ√™ncia autom√°tica\n  let i=0;\n  function next(){\n    if(i>=msgs.length) return;\n    const m = msgs[i++];\n    typing(true);\n    setTimeout(()=>{ typing(false); bubble(m); next(); }, m.delay);\n  }\n  next();\n\n  // Quick replies\n  document.querySelectorAll('.pill').forEach(p=>{\n    p.addEventListener('click', ()=>{\n      bubble({type:'out', text:p.dataset.reply});\n      typing(true);\n      setTimeout(()=>{\n        typing(false);\n        if(p.dataset.reply.includes('agendar')){\n          bubble({type:'in', text:'Perfeito! Esses hor√°rios est√£o livres: 14:30 ‚Ä¢ 16:00 ‚Ä¢ 19:15. Qual prefere?'});\n        } else if(p.dataset.reply.includes('Valores')){\n          bubble({type:'in', text:'Setup √∫nico R$ 5.997 + manuten√ß√£o a partir de R$ 1.997/m√™s. Uma venda paga o m√™s. Quer ativar agora?'});\n        } else {\n          bubble({type:'in', text:'Sem problema. J√° subi seu card pro closer humano e ele te chama ainda hoje ‚úÖ'});\n        }\n      }, 900);\n    });\n  });\n\n  // Captura UTMs + URL\n  const params = new URLSearchParams(window.location.search);\n  const setIf = (id, key) => {\n    const el = document.getElementById(id);\n    if(!el) return;\n    const val = params.get(key) || '';\n    el.value = val;\n  };\n  (function capture(){\n    const urlEl = document.getElementById('page_url');\n    if(urlEl) urlEl.value = window.location.href;\n    setIf('utm_source','utm_source');\n    setIf('utm_medium','utm_medium');\n    setIf('utm_campaign','utm_campaign');\n    setIf('utm_term','utm_term');\n    setIf('utm_content','utm_content');\n  })();\n\n  // UX do formul√°rio: desabilita bot√£o ao enviar\n  const form = document.getElementById('leadForm');\n  if(form){\n    form.addEventListener('submit', function(){\n      const btn = form.querySelector('button[type=\"submit\"]');\n      if(btn){\n        btn.setAttribute('disa